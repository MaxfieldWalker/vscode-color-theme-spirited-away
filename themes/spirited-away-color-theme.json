{
    "name": "Spirited Away",
    "tokenColors": [
        {
            "settings": {
                "background": "#151515",
                "foreground": "#CFCFCF"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#6d966c"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "constant.character.escape.ts"
            ],
            "settings": {
                "foreground": "#e7ab65"
            }
        },
        {
            "name": "Regex",
            "scope": [
                "constant.other.character-class.regexp",
                "constant.other.character-class.set.regexp",
                "constant.character.escape.backslash.regexp"
            ],
            "settings": {
                "foreground": "#e7ab65"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#AFC563"
            }
        },
        {
            "name": "Built-in constant. e.g. 'undefined', 'true', 'false'",
            "scope": "constant.language",
            "settings": {
                "foreground": "#268ac3"
            }
        },
        {
            "name": "Keyword. e.g. 'export', 'return'",
            "scope": "keyword",
            "settings": {
                "foreground": "#ef6363"
            }
        },
        {
            "name": "Storage. e.g. '=', '<<'",
            "scope": "storage",
            "settings": {
                "foreground": "#CFCFCF"
            }
        },
        {
            "name": "Storage modifier. e.g. 'public'",
            "scope": "storage.modifier",
            "settings": {
                "foreground": "#268ac3"
            }
        },
        {
            "name": "'this' 'super'",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "#268ac3"
            }
        },
        {
            "name": "Storage type. e.g. 'function', 'let', 'const'",
            "scope": "storage.type",
            "settings": {
                "foreground": "#268ac3"
            }
        },
        {
            "name": "Arrow",
            "scope": "storage.type.function.arrow",
            "settings": {
                "foreground": "#ef6363"
            }
        },
        {
            "name": "Class name. e.g. 'Map', Action<'T'>",
            "scope": [
                "entity.name.class",
                "entity.name.type.class",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#ef6363"
            }
        },
        {
            "name": "Entity name type",
            "scope": "entity.name.type",
            "settings": {
                "foreground": "#ef6363"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#da846b"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#CFCFCF"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "",
                "foreground": "#5cafdf"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "variable.other.readwrite"
            ],
            "settings": {
                "foreground": "#CFCFCF"
            }
        },
        {
            "name": "Property",
            "scope": [
                "support.variable.property",
                "variable.property",
                "variable.object.property",
                "meta.definition.property",
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#CFCFCF"
            }
        },
        {
            "name": "Imported item",
            "scope": [
                "variable.other.readwrite.alias"
            ],
            "settings": {
                "foreground": "#CFCFCF"
            }
        },
        {
            "name": "Enum",
            "scope": [
                "variable.other.enummember"
            ],
            "settings": {
                "foreground": "#AFC563"
            }
        },
        {
            "name": "Library class/type. e.g. 'string', 'number'",
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#ef6363"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "e.g. name':' string, string '|' number",
            "scope": "keyword.operator.type",
            "settings": {
                "foreground": "#CFCFCF"
            }
        },
        {
            "name": "'new', 'of'",
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression"
            ],
            "settings": {
                "foreground": "#268ac3"
            }
        },
        {
            "name": "Markdown header",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#ef6363"
            }
        },
        {
            "name": "Markdown bold",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#268ac3"
            }
        },
        {
            "name": "Markdown list",
            "scope": "beginning.punctuation.definition.list",
            "settings": {
                "foreground": "#ef6363"
            }
        },
        {
            "name": "Markdown link",
            "scope": "markup.underline.link",
            "settings": {
                "foreground": "#5cafdf"
            }
        },
        {
            "name": "Markdown inline code",
            "scope": [
                "markup.inline.raw",
                "punctuation.definition.raw"
            ],
            "settings": {
                "foreground": "#268ac3"
            }
        },
        {
            "name": "Markdown code block",
            "scope": [
                "punctuation.definition.markdown",
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#da846b"
            }
        }
    ],
