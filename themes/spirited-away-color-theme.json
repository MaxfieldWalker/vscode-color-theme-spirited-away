{
    "name": "Spirited Away",
    "tokenColors": [
        {
            "settings": {
                "background": "#151515",
                "foreground": "#CFCFCF"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#6d966c"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "constant.character.escape.ts"
            ],
            "settings": {
                "foreground": "#e7ab65"
            }
        },
        {
            "name": "Regex",
            "scope": [
                "constant.other.character-class.regexp",
                "constant.other.character-class.set.regexp",
                "constant.character.escape.backslash.regexp"
            ],
            "settings": {
                "foreground": "#e7ab65"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#AFC563"
            }
        },
        {
            "name": "Built-in constant. e.g. 'undefined', 'true', 'false'",
            "scope": "constant.language",
            "settings": {
                "foreground": "#268ac3"
            }
        },
        {
            "name": "Keyword. e.g. 'export', 'return'",
            "scope": "keyword",
            "settings": {
                "foreground": "#ef6363"
            }
        },
        {
            "name": "Storage. e.g. '=', '<<'",
            "scope": "storage",
            "settings": {
                "foreground": "#CFCFCF"
            }
        },
        {
            "name": "Storage modifier. e.g. 'public'",
            "scope": "storage.modifier",
            "settings": {
                "foreground": "#268ac3"
            }
        },
        {
            "name": "'this' 'super'",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "#268ac3"
            }
        },
        {
            "name": "Storage type. e.g. 'function', 'let', 'const'",
            "scope": "storage.type",
            "settings": {
                "foreground": "#268ac3"
            }
        },
        {
            "name": "Arrow",
            "scope": "storage.type.function.arrow",
            "settings": {
                "foreground": "#ef6363"
            }
        },
        {
            "name": "Class name. e.g. 'Map', Action<'T'>",
            "scope": [
                "entity.name.class",
                "entity.name.type.class",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#ef6363"
            }
        },
        {
            "name": "Entity name type",
            "scope": "entity.name.type",
            "settings": {
                "foreground": "#ef6363"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#d17e66"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#CFCFCF"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#5cafdf"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "",
                "foreground": "#5cafdf"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "variable.other.readwrite"
            ],
            "settings": {
                "foreground": "#5cafdf"
            }
        },
        {
            "name": "Property",
            "scope": [
                "support.variable.property",
                "variable.property",
                "variable.object.property",
                "meta.definition.property",
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#5cafdf"
            }
        },
        {
            "name": "Imported item",
            "scope": [
                "variable.other.readwrite.alias"
            ],
            "settings": {
                "foreground": "#5cafdf"
            }
        },
        {
            "name": "Enum",
            "scope": [
                "variable.other.enummember"
            ],
            "settings": {
                "foreground": "#AFC563"
            }
        },
        {
            "name": "Library class/type. e.g. 'string', 'number'",
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#ef6363"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "e.g. name':' string, string '|' number",
            "scope": "keyword.operator.type",
            "settings": {
                "foreground": "#CFCFCF"
            }
        },
        {
            "name": "'new'",
            "scope": "keyword.operator.new",
            "settings": {
                "foreground": "#268ac3"
            }
        },
        {
            "name": "Markdown header",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#ef6363"
            }
        },
        {
            "name": "Markdown bold",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#268ac3"
            }
        },
        {
            "name": "Markdown list",
            "scope": "beginning.punctuation.definition.list",
            "settings": {
                "foreground": "#ef6363"
            }
        },
        {
            "name": "Markdown link",
            "scope": "markup.underline.link",
            "settings": {
                "foreground": "#5cafdf"
            }
        },
        {
            "name": "Markdown inline code",
            "scope": [
                "markup.inline.raw",
                "punctuation.definition.raw"
            ],
            "settings": {
                "foreground": "#268ac3"
            }
        },
        {
            "name": "Markdown code block",
            "scope": [
                "punctuation.definition.markdown",
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#d17e66"
            }
        }
    ],
    "colors": {
        // Base Colors

        // 入力ボックスの背景
        "input.background": "#222222",
        // 入力ボックスのアクティブ時のボーダー
        "inputOption.activeBorder": "#EC5151",

        // ドロップダウンの背景
        "dropdown.background": "#151515",

        // エディタの背景
        "editor.background": "#151515",
        "editor.foreground": "#CFCFCF",

        // カーソル
        "editorCursor.foreground": "#BD3538",

        // 空白
        "editorWhitespace.foreground": "#505050",

        // 行の強調表示
        "editor.lineHighlightBackground": "#222222",

        // 選択
        "editor.selectionBackground": "#CFCFCF20",
        "editor.inactiveSelectionBackground": "#CFCFCF18",

        // インデントガイド
        "editorIndentGuide.background": "#505050",

        // ホバー表示の背景
        "editorHoverWidget.background": "#000000",
        // ホバー表示のボーダー
        "editorHoverWidget.border": "#505050",

        "peekViewResult.background": "#000000",
        "peekViewResult.selectionBackground": "#EC5151",
        "peekViewEditor.background": "#151515",
        "peekViewTitle.background": "#000000",
        "peekView.border": "#505050",
        "peekViewEditor.matchHighlightBackground": "#AFC56350",

        // 行番号
        "editorLineNumber.foreground": "#505050",

        "editorWidget.background": "#000000",
        "editor.findMatchBackground": "#CFCFCF50",
        "editor.findMatchHighlightBackground": "#CFCFCF30",
        "editor.findRangeHighlightBackground": "#000000",


        // タブコンテナの背景
        "editorGroupHeader.tabsBackground": "#151515",
        "editorGroupHeader.noTabsBackground": "#151515",

        // タブのボーダー
        "tab.border": "#000000",
        // アクティブなタブの背景
        "tab.activeBackground": "#000000",
        // アクティブでないタブの背景
        "tab.inactiveBackground": "#222222",


        // エディタグループのボーダー
        "editorGroup.border": "#000000",
        // エディタグループの背景
        "editorGroup.background": "#151515",

        // エディターにファイルなどをドラッグアンドドロップした時の背景
        "editorGroup.dropBackground": "#CFCFCF20",

        // アクティビティバーにファイルなどをドラッグアンドドロップした時の背景
        "activityBar.dropBackground": "#CFCFCF20",

        // パネルの上側のボーダー
        "panel.border": "#000000",

        // ステータスバーの背景
        "statusBar.background": "#EC5151",
        // ステータスバーのWorkspaceが無い場合の背景
        "statusBar.noFolderBackground": "#151515",
        // ステータスバーのデバッグ中の背景
        "statusBar.debuggingBackground": "#BD3538",


        // デバッグツールバーの背景
        "debugToolBar.background": "#000000",

        // アクティビティバーの背景
        "activityBar.background": "#151515",

        // アクティビティバーのバッジの背景
        "activityBarBadge.background": "#EC5151",

        // サイドバーの背景
        "sideBar.background": "#000000",

        // タイトルバーのアクティブ時の背景
        "titleBar.activeBackground": "#000000",
        // タイトルバーの非アクティビティ時の背景
        "titleBar.inactiveBackground": "#151515"
    }
}